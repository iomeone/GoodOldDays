% -----------------------------------------------------------------------------
% Tunable Parameters:
% The width of a line representing a task
/width 50 def
% Scaling factor for the y-axis (usful to enlarge)
/y-scale 1 def
% -----------------------------------------------------------------------------
/total-len 283149 def
/show-len 790 def
/x-offset 100 def
/y-offset 50 def
% normalize is the PS version of the formula: 
%   int((/283149) * (790-100) + 100) 
% in psout.
/normalize { total-len div show-len x-offset sub mul x-offset add floor } def
/x-normalize { exch show-len mul total-len div exch } def
/y-normalize { y-offset sub y-scale mul y-offset add } def
/str-len 12 def
/prt-n { cvi str-len string cvs 
         dup stringwidth pop 
         currentpoint pop 780 gt { 10 sub } { 2 div } ifelse 
         neg 0 rmoveto 
         show  } def 
        % print top-of-stack integer centered at the current point
/R { 0.8 0 0 } def
/G { 0 0.9 0.1 } def
/B { 0 0.1 0.9 } def
/set-bg { setrgbcolor } def
% RL: runlineto; draws a horizontal line in given color
% Operands: x-from x-to y color
/RL { set-bg   % set color 
      newpath y-normalize  % mangle y val
      2 index 1 index moveto width setlinewidth 
      lineto pop stroke} def
% SL: suspendlineto; draws a horizontal line in given color (thinner)
% Operands: x-from x-to y color
/SL { set-bg   % set color 
      newpath y-normalize  % mangle y val
      2 index 1 index moveto width 2 div setlinewidth 
      lineto pop stroke} def
% FL: fetchlineto; draws a horizontal line in given color (thinner)
% Operands: x-from x-to y color
/FL { set-bg   % set color 
      newpath y-normalize  % mangle y val
      2 index 1 index moveto width  2  div setlinewidth 
      lineto pop stroke} def
/printText { 0 0 moveto (GrAnSim) show } def
/asciilogo { 5 sub moveto HB16 setfont (GrAnSim) show } def
/logo { gsave 
        translate 
        .95 -.05 0
          { dup 1 exch sub 0 exch setrgbcolor printText 1 -.5 translate } for 
        1 0 0 setrgbcolor printText
        grestore} def
