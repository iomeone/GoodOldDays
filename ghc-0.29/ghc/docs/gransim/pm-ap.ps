/R { 0.8 0 0 } def
/G { 0 0.9 0.1 } def
/B { 0 0.1 0.9 } def
/set-bg { setrgbcolor } def
% RL: runlineto; draws a horizontal line in given color
% Operands: x-from x-to y color
/RL { set-bg   % set color 
      newpath y-normalize  % mangle y val
      2 index 1 index moveto width setlinewidth 
      lineto pop stroke} def
% SL: suspendlineto; draws a horizontal line in given color (thinner)
% Operands: x-from x-to y color
/SL { set-bg   % set color 
      newpath y-normalize  % mangle y val
      2 index 1 index moveto width 2 div setlinewidth 
      lineto pop stroke} def
% FL: fetchlineto; draws a horizontal line in given color (thinner)
% Operands: x-from x-to y color
/FL { set-bg   % set color 
      newpath y-normalize  % mangle y val
      2 index 1 index moveto width  2  div setlinewidth 
      lineto pop stroke} def
/printText { 0 0 moveto (GrAnSim) show } def
/asciilogo { 5 sub moveto HB16 setfont (GrAnSim) show } def
/logo { gsave 
        translate 
        .95 -.05 0
          { dup 1 exch sub 0 exch setrgbcolor printText 1 -.5 translate } for 
        1 0 0 setrgbcolor printText
        grestore} def
